<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duration__c</fullName>
    <description>Calculated duration of a work day in decimal hour format (3 hours 30 minutes = 3,5)</description>
    <externalId>false</externalId>
    <formula>(HOUR(DayEndTime__c) - HOUR(DayStartTime__c)) + (MINUTE(DayEndTime__c)-MINUTE(DayStartTime__c)) / 60</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated duration for a regular work day</inlineHelpText>
    <label>Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
