<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CustomerIdNumber__c</fullName>
    <description>Converts the text customer id field to a number. This field is used in SOQL to determine the next customer id.</description>
    <externalId>false</externalId>
    <formula>IF(ISNUMBER(CustomerId__c), VALUE(CustomerId__c), null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Customer Id Number</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
